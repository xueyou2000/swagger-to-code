{
	"name": "swagger-to-code",
	"displayName": "swagger-to-code",
	"description": "swagger文档代码生成",
	"version": "0.0.1",
	"icon": "./assets/logo.png",
	"publisher": "XueYou",
	"license": "MIT",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/vscode-plugin/extension.js",
	"contributes": {
		"jsonValidation": [
			{
				"fileMatch": "swagger.json",
				"url": "./assets/config-schema.json"
			}
		],
		"commands": [
			{
				"command": "swagger.findInterface",
				"title": "Swagger: find interface (寻找接口)"
			},
			{
				"command": "swagger.findEntity",
				"title": "Swagger find entity (寻找实体)"
			}
		],
		"keybindings": [
			{
				"command": "swagger.findInterface",
				"key": "ctrl+alt+i",
				"mac": "cmd+alt+i"
			},
			{
				"command": "swagger.findEntity",
				"key": "ctrl+alt+e",
				"mac": "cmd+alt+e"
			}
		],
		"configuration": {
			"title": "SwaggerToCode",
			"properties": {
				"swagger.filename": {
					"type": [
						"string"
					],
					"default": "swagger.json",
					"desc": "swagger配置文件名称"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xueyou2000/swagger-to-code.git"
	}
}